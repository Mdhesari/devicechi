{
    "name": "laravel/laravel",
    "type": "project",
    "description": "Mobileforsale a marketplace platform for selling mobile devices.",
    "keywords": [
        "mobile for sale",
        "mfs",
        "marketplace",
        "sell and buy",
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "alexusmai/laravel-file-manager": "^2.5",
        "artesaos/seotools": "^0.20.0",
        "cviebrock/eloquent-sluggable": "^8.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "ghasedak/laravel": "^1.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "hekmatinasser/verta": "^1.11",
        "inertiajs/inertia-laravel": "^0.2.15",
        "intervention/image": "^2.5",
        "kalnoy/nestedset": "^5.0",
        "kg-bot/laravel-localization-to-vue": "^1.6",
        "laravel/framework": "^8.0",
        "laravel/horizon": "^5.3",
        "laravel/jetstream": "^1.3",
        "laravel/sanctum": "^2.6",
        "laravel/tinker": "^2.0",
        "nwidart/laravel-modules": "^8.0",
        "spatie/laravel-activitylog": "^3.16",
        "spatie/laravel-medialibrary": "^9.0.0",
        "spatie/laravel-permission": "^3.18",
        "spatie/laravel-settings": "^1.0",
        "spatie/laravel-sitemap": "^5.9",
        "tightenco/ziggy": "^1.0",
        "laravel/telescope": "^4.2",
        "fabpot/goutte": "^4.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.8",
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "laravel/sail": "^1.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope",
                "ghasedak/laravel"
            ]
        }
    },
    "autoload": {
        "files": [
            "Helpers/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Leantony\\Grid\\": "Packages/Leantony/Grid/src",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Modules\\User\\Tests\\": "tests/",
            "Modules\\Team\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
